{
  "dotvvmVersion": "*.*.*.*",
  "config": {
    "applicationPhysicalPath": "/opt/myApp",
    "markup": {
      "ViewCompilation": {
        "mode": "Lazy",
        "backgroundCompilationDelay": "00:00:30",
        "compileInParallel": false
      }
    },
    "resources": {},
    "security": {},
    "runtime": {
      "maxPostbackSizeBytes": 100
    },
    "defaultCulture": "cs-CZ",
    "clientSideValidation": false,
    "experimentalFeatures": {},
    "debug": true,
    "diagnostics": {
      "compilationPage": {},
      "perfWarnings": {}
    }
  }
}
